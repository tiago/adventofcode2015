{
  "name": "adventofcode2015",
  "version": "1.0.0",
  "description": "Solutions for adventofcode.com",
  "main": "index.js",
  "devDependencies": {
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.1",
    "md5": "^2.0.0",
    "mocha": "^2.3.4"
  },
  "scripts": {
    "test": "mocha ./**/*.spec.js",
    "tdd": "npm test -- --watch",
    "cover": "istanbul cover _mocha ./day*/*.spec.js -x *.spec.js",
    "lint": "eslint ./index.js ./day*/*.js",
    "check": "npm run lint && npm run cover",
    "clean": "rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/tiago/adventofcode2015.git"
  },
  "author": "Tiago Oliveira",
  "license": "ISC"
}
